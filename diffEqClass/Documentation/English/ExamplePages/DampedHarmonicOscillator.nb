Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"M", " ", 
     OverscriptBox["x", ".."]}], "+", 
    RowBox[{"\[Gamma]", " ", 
     OverscriptBox["x", "."]}], "+", 
    RowBox[{"k", " ", "x"}]}], "\[Equal]", 
   RowBox[{"F", "(", "t", ")"}]}], 
