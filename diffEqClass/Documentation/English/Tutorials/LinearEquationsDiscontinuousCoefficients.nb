Cell[TextData[{
 StyleBox["Theorem.",
  FontWeight->"Bold"],
 StyleBox[" (The Principle of Superposition.)",
  FontSlant->"Plain"],
 "  If ",
 Cell[BoxData[
  FormBox[
   RowBox[{
