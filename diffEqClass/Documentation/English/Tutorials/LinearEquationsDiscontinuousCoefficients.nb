Cell[TextData[{
 "Thus the function ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[CurlyPhi]", "x"], "(", 
    SubscriptBox["y", "0"], ")"}], TraditionalForm]],ExpressionUUID->
  "5314d1b5-ce8f-4bfa-8e0a-3064ba9f83df"],
